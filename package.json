{
  "name": "http-respond",
  "version": "1.0.0",
  "description": "simple http final responder",
  "main": "index.js",
  "scripts": {
    "start": "node-dev example",
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test run-default tape test/*.js --",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/http-respond.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/http-respond/issues"
  },
  "homepage": "https://github.com/ahdinosaur/http-respond#readme",
  "devDependencies": {
    "dependency-check": "^2.7.0",
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "run-default": "^1.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "es2040": "^1.2.3",
    "fast-safe-stringify": "^1.1.6",
    "finalhandler": "^0.5.1",
    "from2-string": "^1.1.0",
    "http-errors": "^1.5.1",
    "is-pull-stream": "0.0.0",
    "is-stream": "^1.1.0",
    "pull-stream-to-stream": "^1.3.3",
    "pump": "^1.0.2",
    "server-sink": "^1.0.0"
  }
}
